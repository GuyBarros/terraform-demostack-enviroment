#####################
#TFE Settings
#####################
TFE_ORGANIZATION = "GuyBarros"
#####################
# Base
#####################
TFE_AWS_DEMOSTACK_WORKSPACE = "GUY-AWS-Demostack"
TTL                         = "72"
owner                       = "guybarros"
servers                     = "3"
workers                     = "3"
