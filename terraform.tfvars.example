cni_plugin_url = "https://github.com/containernetworking/plugins/releases/download/v0.8.6/cni-plugins-linux-amd64-v0.8.6.tgz"
#####################
# TLS_ROOT
#####################
TFE_TLS_ROOT_WORKSPACE = "tls-root-certificate"
validity_period_hours  = "8760"
organization           = "HashiCorp Emea Demostack"
is_ca_certificate      = "true"
ecdsa_curve            = "P521"
common_name            = "service.consul"
algorithm              = "ECDSA"
#####################
# TLS
#####################
ca_key_algorithm      = ""
ca_private_key_pem    = ""
ca_cert_pem           = ""
consul_gossip_key     = ""
consul_master_token   = ""
consul_join_tag_value = ""
nomad_gossip_key      = ""
#####################
#AWS
#####################
primary_namespace    = "EU-grstack"
